{% if data['provider'] == 'Ambition' %}
  
  <form method="post">
      {{ govukRadios({
        name: "[newDeliveryPartner]",
        value: data.newDeliveryPartner,
        items: [
          {
            value: "Beatty-Mayert",
            text: "Beatty-Mayert"
          },
          {
            value: "Borer-Wintheiser",
            text: "Borer-Wintheiser"
          },
          {
            value: "Connelly-Marquardt",
            text: "Connelly-Marquardt"
          }
        ]
      }) }}
      {{ govukButton({
        text: "Continue"
      }) }}
    </form>   


{% elseif data['provider'] == 'Capita' %}
  

  <form method="post">
      {{ govukRadios({
        name: "[newDeliveryPartner]",
        value: data.newDeliveryPartner,
        items: [
          {
            value: "Brakus-Turcotte",
            text: "Brakus-Turcotte"
          },
          {
            value: "Krajcik-Bradtke",
            text: "Krajcik-Bradtke"
          },
          {
            value: "Labadie, Abshire and Ullrich",
            text: "Labadie, Abshire and Ullrich"
          },
          {
